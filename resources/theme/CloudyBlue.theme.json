{
  "name": "Cloudy Blue",
  "dark": false,
  "author": "Chris Sinco",
  "parentTheme": "ExperimentalLight",
  "nameKey": "cloudyblue.theme.name",
  "editorScheme": "Cloudy Blue",
  "colors": {
    "Seed": "#4285F4",
    "Debug": "#ff00ff",
    "PrimaryBackground": "#005AC1",
    "ContentBackground": "#FEFBFF",
    "SecondaryBackground": "#F2F4FF",
    "MainToolbarBackground": "#001A41",
    "MainToolbarForeground": "#d8e2ff",
    "HoverBackground": "#d8e2ff",
    "SeparatorFill": "#74777F",
    "BorderColor": "#D8E2FF",
    "SelectionActive": "#D8E2FF",
    "SelectionInactive": "#dbe2f9",
    "SelectionForeground": "#001a41",
    "Foreground": "#1B1B1F",
    "SecondaryForeground": "#575e71",
    "DisabledForeground": "#6F778B",
    "InverseSurface": "#2f3036",
    "InverseOnSurface": "#f0f0f7",
    "InverseOnSurfaceSecondary": "#c4c6d0",
    "InversePrimary": "#adc6ff",
    "Outline": "#74777f"
  },
  "icons": {
    "ColorPalette": {

      "Gray.Stroke": "#005AC1",
      "Gray.Fill": "#D8E2FF"
    }
  },
  "ui": {
    "*": {
      "background": "SecondaryBackground",
      "borderColor": "BorderColor",
      "selectionBackground": "SelectionActive",
      "selectionInactiveBackground": "SelectionInactive",
      "selectionForeground": "SelectionForeground",
      "selectionInactiveForeground": "SelectionForeground",
      "hoverBackground": "#D8E2FF",
      "infoForeground": "SecondaryForeground",
      "disabledForeground": "DisabledForeground",
      "inactiveForeground": "DisabledForeground",
      "acceleratorForeground": "DisabledForeground",
      "acceleratorSelectionForeground": "SecondaryForeground",
      "shortcutForeground": "DisabledForeground",
      "separatorColor": "BorderColor",
      "underlineColor": "PrimaryBackground",
      "inactiveUnderlineColor": "#BFC6DC"
    },
    "ActionButton": {
      "hoverBackground": "HoverBackground",
      "hoverBorderColor": "#00000000",
      "pressedBackground": "SelectionInactive",
      "pressedBorderColor": "#00000000",
      "hoverSeparatorColor": "BorderColor"
    },
    "Borders.color": "BorderColor",
    "Button.background": "PrimaryBackground",
    "CompletionPopup": {
      "foreground": "Foreground",
      "matchForeground": "PrimaryBackground",
      "Advertiser": {
        "foreground": "DisabledForeground",
        "background": "SecondaryBackground"
      }
    },
    "DefaultTabs": {
      "background": "ContentBackground",
      "underlinedTabBackground": "ContentBackground",
      "underlinedTabForeground": "Foreground"
    },
    "EditorTabs": {
      "background": "ContentBackground",
      "underlinedTabBackground": "ContentBackground",
      "underlinedTabForeground": "Foreground",
      "hoverBackground": "#00000000"
    },
    "Editor": {
      "SearchField": {
        "background": "SecondaryBackground"
      },
      "Toolbar": {
        "borderColor": "BorderColor"
      }
    },
    "MainToolbar": {
      "background": "MainToolbarBackground",
      "foreground": "MainToolbarForeground",
      "inactiveBackground": "#001E49",
      "separatorColor": "BorderColor"
    },
    "MainWindow.Tab": {
      "selectedForeground": "MainToolbarForeground",
      "selectedBackground": "MainToolbarBackground",
      "selectedInactiveBackground": "#001E49",
      "foreground": "InversePrimary",
      "background": "#00285D",
      "hoverBackground": "#001E49",
      "separatorColor": "#004494",
      "borderColor": "#003271"
    },
    "StatusBar": {
      "Widget": {
        "foreground": "DisabledForeground",
        "hoverForeground": "Foreground",
        "pressedBackground": "SelectionInactive"
      },
      "Breadcrumbs": {
        "foreground": "DisabledForeground",
        "hoverForeground": "Foreground"
      }
    },
    "ToolTip": {
      "background": "MainToolbarBackground",
      "foreground": "InverseOnSurface",
      "infoForeground": "InverseOnSurfaceSecondary",
      "shortcutForeground": "InverseOnSurfaceSecondary",
      "linkForeground": "InversePrimary",
      "borderColor": "Outline"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "SecondaryBackground"
      },
      "Button": {
        "hoverBackground": "HoverBackground",
        "selectedBackground": "PrimaryBackground"
      },
      "Stripe": {
        "separatorColor": "BorderColor",
        "DragAndDrop": {
          "separatorColor": "BorderColor"
        }
      }
    }
  }
}